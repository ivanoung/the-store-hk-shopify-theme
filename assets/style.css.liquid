  right:auto;
  direction:ltr;
}
.navbar .navbar-nav.navbar-left>li.dropdown-grid>.dropdown-grid-wrapper.dropdown-menu-right, .navbar .navbar-nav.navbar-right>li.dropdown-grid>.dropdown-grid-wrapper {
